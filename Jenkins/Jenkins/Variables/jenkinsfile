pipeline {
    agent any 

    environment {
            def myString = "hello world"
            def myNumber = 10
            def myBool = true
    }
    stages{
        stage(""){
            steps {
                echo "myS: ${myString}"
                 echo "myn: ${myNumber}"
                  echo "myB: ${myBool}"

            }
        }
    }
}